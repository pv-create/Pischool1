<div class="container">
    <h1>Панель администратора</h1>
   

    <table class="table">
  <thead>
     Пользователи
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Email</th>
      <th scope="col">Phonenumber</th>
      <th scope="col">Изменить</th>
    </tr>
  </thead>
  <tbody>
      @foreach (var b in  ViewBag.Users)
      {
         <tr>
            @*<th scope="row">1</th>*@
            <td>@b.Id</td>
            <td>@b.Email</td>
            <td>@b.PhoneNumber</td>
            <td><button class="btn btn-primary" type="submit">Изменить</button></td>
        </tr>
       }
  </tbody>
</table>


























